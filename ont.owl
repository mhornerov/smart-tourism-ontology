    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/smartourism#price -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/smartourism#price">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Hotel"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Restaurant"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Vehicle"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The price of the item.</rdfs:comment>
        <rdfs:label>price</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/smartourism#ratingValue -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/smartourism#ratingValue">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topDataProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Rating"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>The rating for the content.</rdfs:comment>
        <rdfs:label>ratingValue</rdfs:label>
    </owl:DatatypeProperty>
    


