        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>An official rating for a lodging business or food establishment, e.g. from national associations or standards bodies.</rdfs:comment>
        <rdfs:label>starRating</rdfs:label>
        <rdfs:sameAs>https://schema.org/starRating</rdfs:sameAs>
