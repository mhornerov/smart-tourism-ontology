        <rdfs:label>servesCuisine</rdfs:label>
        <rdfs:sameAs>https://schema.org/servesCuisine</rdfs:sameAs>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/smartourism#starRating -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/smartourism#starRating">
