    </owl:Class>
    


    <!-- https://w3id.org/smartourism#Event -->


    <owl:Class rdf:about="https://w3id.org/smartourism#Event">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/smartourism#locationCity"/>
                <owl:someValuesFrom rdf:resource="https://w3id.org/smartourism#City"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/smartourism#startDate"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>An event happening at a certain time and location, such as a concert, lecture, or festival.</rdfs:comment>
        <rdfs:isDefinedBy>https://schema.org/Event</rdfs:isDefinedBy>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


