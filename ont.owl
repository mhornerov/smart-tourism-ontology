<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/smartourism#"
     xml:base="https://w3id.org/smartourism"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:smartourism="https://w3id.org/smartourism#">
    <owl:Ontology rdf:about="https://w3id.org/smartourism">
        <owl:versionIRI rdf:resource="https://w3id.org/smartourism/1.0.0"/>
        <dc:abstract>The domain of the ontology is framed around the different activities that surround traveling to a different city. The scope of the ontology will be modelled on the touristic attractions, events, hotels, restaurants and means of transport in a certain city.</dc:abstract>
        <dc:description>This ontology structures information about events, hotels, tourist attractions and restaurants in different cities; shaping the domain of smart tourism. Its objective is to facilitate the semantic representation of the tourist offer to allow personalised recommendations and advanced queries by tourists and smart tourism systems.</dc:description>
        <dc:title>Smart Tourism Ontology</dc:title>
        <rdfs:comment>A Smart Tourism and Recommendations ontology.</rdfs:comment>
        <rdfs:label>SmartTourism</rdfs:label>
        <owl:versionInfo>Initial version of the ontology with the integration of,
- CSVs with information of events, hotels, attractions, restaurants, means of transport and cities</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://purl.org/dc/elements/1.1/abstract -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/abstract"/>
    


    <!-- http://purl.org/dc/elements/1.1/description -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/description"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->


    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#sameAs -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/smartourism#locationCity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/smartourism#locationCity">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Event"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Hotel"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Restaurant"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#TouristAttraction"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Vehicle"/>
        <rdfs:range rdf:resource="https://w3id.org/smartourism#City"/>
        <rdfs:comment>Information about location.</rdfs:comment>
        <rdfs:isDefinedBy>https://dbpedia.org/property/locationCity</rdfs:isDefinedBy>
        <rdfs:label>locationCity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/smartourism#rating -->


    <owl:ObjectProperty rdf:about="https://w3id.org/smartourism#rating">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Hotel"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#Restaurant"/>
        <rdfs:domain rdf:resource="https://w3id.org/smartourism#TouristAttraction"/>
        <rdfs:range rdf:resource="https://w3id.org/smartourism#Rating"/>
        <rdfs:comment>The value of the evaluation.</rdfs:comment>
        <rdfs:label>rating</rdfs:label>
    </owl:ObjectProperty>
    


